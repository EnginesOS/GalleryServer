<!DOCTYPE html>
<html>
<head>
  <title>EngineOSGallery</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render partial: 'layouts/devise_navbar' %>
<div class="content">
  <div class="container">
    <div class="row">
      <div class="col-sm-6 col-sm-offset-3 devise-view">
        <%= yield %>
      </div>
    </div>      
  </div>
</div>

<div id="footer">
</div>

<%= render partial: 'shared/gadgets/waiting_for_response' %>

</body>
</html>
